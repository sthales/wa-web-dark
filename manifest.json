{
  "name": "DarkWaWeb",
  "version": "0.5",
  "description": "Forces Dark theme (beta) on WhatsApp Web",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "dark-wa-logo.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"]
}
